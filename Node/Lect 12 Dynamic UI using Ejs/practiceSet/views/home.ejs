<%- include("partials/head") %>
<body>
    <%- include("partials/nav") %>
    <section class="border">
        <h1>Here are our register Homes</h1>
        <ul>
            <% registerHomes.forEach(home => { %>
                <li>
                    <div class="card" style="width: 18rem;">
                        <img src="lkasdlkdsja" class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title"><%= home.houseName %></h5>
                            <p class="card-text"><%= home.location %></p>
                            <p class="card-text"><%= home.Price %></p>
                            <a href="#" class="btn btn-primary"><%= home.rating %></a>
                        </div>
                    </div>
                </li>
            <% }) %>
        </ul>
        <a href="/add-home" class="">Add Home</a>    
    </section>
</body>
</html> 